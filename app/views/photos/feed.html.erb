<h1>List of photos</h1>

<hr>
  <div>
    <div>
      <table border="1">
        <tr>
          <th>Image</th>
          <th>Caption</th>
          <th>Owner</th>
          <th>Posted</th>
          <th>Likes</th>
          <th></th>
        </tr>

        <% @feed.each do |a_photo| %>
        <tr>
            <td>
              <img src="<%= a_photo.image %>">
            </td>
          
            <td>
             <%= a_photo.caption %>
            </td>
          
            <td>
              catherina
            </td>
          
            <td>
              <%= a_photo.created_at %>
            </td>
          
            <td>
              <%= a_photo.likes_count %>
            </td>
          
            <td>
              <a href="/photos/777">
                Show details
              </a>
            </td>
          </tr>
          <% end %>
      </table>
    </div>
  </div>
</hr> 
